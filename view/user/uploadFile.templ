package user

templ UploadFile(){
    <div id="upload-container">
        <form id="audio-form" hx-post="/upload-file" 
              hx-swap="beforeend"
              hx-target="#file-list"
              hx-encoding="multipart/form-data" >
            <input type="file" name="audio-file" id="audio-file"/>
            <button type="submit"> Upload </button>
        </form>

    </div>
}
